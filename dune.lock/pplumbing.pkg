(version 0.0.14)

(build
 (progn
  (when
   %{pkg-self:dev}
   (run dune subst))
  (run dune build -p %{pkg-self:name} -j %{jobs} @install)))

(depends
 dune
 ocaml
 cmdlang
 cmdlang-to-cmdliner
 cmdliner
 dyn
 fmt
 loc
 logs
 ordering
 parsexp
 pp
 sexplib0
 stdune)

(source
 (fetch
